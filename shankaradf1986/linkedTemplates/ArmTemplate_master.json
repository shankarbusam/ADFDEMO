{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shankaradf1986"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlsalesDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlsalesDatabase'"
		},
		"foodreviewsdestination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'foodreviewsdestination'"
		},
		"foodreviewssource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'foodreviewssource'"
		},
		"linkAzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkAzureBlobStorage'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shankaradf1986_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlsalesDatabase_connectionString": {
						"value": "[parameters('AzureSqlsalesDatabase_connectionString')]"
					},
					"foodreviewsdestination_connectionString": {
						"value": "[parameters('foodreviewsdestination_connectionString')]"
					},
					"foodreviewssource_connectionString": {
						"value": "[parameters('foodreviewssource_connectionString')]"
					},
					"linkAzureBlobStorage_connectionString": {
						"value": "[parameters('linkAzureBlobStorage_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}